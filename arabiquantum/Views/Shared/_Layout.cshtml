<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - arabiquantum</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/arabiquantum.styles.css" asp-append-version="true" />
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-dark  border-bottom box-shadow mb-0" style="background-color:#1c658f;">
            <div class="container-fluid flex-row-reverse m">
                <a class="navbar-brand " asp-area="" asp-controller="Home" asp-action="Index"><strong>عربي كوانتوم</strong></a>
                <button class="navbar-toggler " type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between  ">
                    <ul class="navbar-nav flex-grow-1 flex-md-row-reverse flex-sm-row-reverse  ">
                        <li class="nav-item ">
                            <a class="nav-link text-light float-end " asp-area="" asp-controller="Home" asp-action="Index">الصفحة الرئيسية</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-light float-end" href="https://arabiquantum.z13.web.core.windows.net/api/index.html">تعلم عن الحوسبة الكمية</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-light float-end" asp-area="" asp-controller="Home" asp-action="Privacy">الخصوصية</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    
            @RenderBody()
       

    <footer class="border-top footer text-light" style="background-color:#1c405d">
        <div class="container">
            &copy; 2023 - arabiquantum - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
