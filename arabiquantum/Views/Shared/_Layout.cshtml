
<!DOCTYPE html>
<html dir="rtl" lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - arabiquantum</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/arabiquantum.styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/homeStyle.css" asp-append-version="true" />
</head>
<body >
    <h2>.</h2>
    
    
    <header>
        <nav class="navbar shadow navbar-expand-sm navbar-toggleable-sm navbar-dark   box-shadow mb-0 fixed-top" style="background-color:#1c658f;">
            <div class="container-fluid  ">
                <a class="navbar-brand border-start ps-3" style=" border-width:3.5px !important;" asp-area="" asp-controller="Home" asp-action="Index"><strong>عربي كوانتوم</strong></a>
                
                <button class="navbar-toggler " type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between ">

                   

                    <ul class="navbar-nav flex-grow-1   ">
                       
                        <li class="nav-item mx-sm-3 mx-xs-2">
                            <a class="nav-link text-light float-end " asp-area="" asp-controller="Home" asp-action="Index">الصفحة الرئيسية</a>
                        </li>
                        <li class="nav-item mx-sm-3 mx-xs-2">
                            <a class="nav-link text-light float-end" href="https://arabiquantum.z13.web.core.windows.net/api/index.html">تعلم عن الحوسبة الكمية</a>
                        </li>
                        <li class="nav-item mx-sm-3 ">
                            <a class="nav-link  text-light float-end" asp-area="" asp-controller="post" asp-action="index">المجتمع</a>
                        </li>
                    </ul>
                </div>
                <div class="box">
                    <form asp-controller="Post" asp-action="index"  method="post" style="margin:auto;max-width:55vh">
                        <input type="text" placeholder="بحث... ">
                        <button type="submit"><i class="fa fa-search"></i></button>
                    </form>
                     
                </div>
            </div>
        </nav>
    </header>   
            @RenderBody()
            <h2 style=" color:white">.</h2>
    <footer class="border-top footer text-light mt-5" style="background-color:#1c405d">
        <div class="container">
            &copy; 2023 - arabiquantum - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.3/umd/popper.min.js" integrity="sha384-vFJXuSJphROIrBnz7yo7oB41mKfc8JzQZiCq4NCceLEaO4IHwicKwpJf9c9IpFgh" crossorigin="anonymous"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta.2/js/bootstrap.min.js" integrity="sha384-alpBpkh1PFOepccYVYDB4do5UnbKysX5WZXm3XxPqe5iKTfUKjNkCk9SaVuEZflJ" crossorigin="anonymous"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
